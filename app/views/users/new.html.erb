<%= form_with model: @user do |form| %>
  <%= form.label :email %>
  <%= form.text_field :email %>
  <%= form.label :username %>
  <%= form.text_field :username %>
  <%= form.label :password %>
  <%= form.password_field :password %>
  <%= form.hidden_field :user_id, value: 1 %>
  <%= form.submit "Create User", data: { disable_with: "Creating User" } %>
<% end %>
